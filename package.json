  "engines": {
    "node": "24.6.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "brew services stop mysql && brew services start mysql && vite",
    "build": "vite build",
    "preview": "vite preview",
    "mysql": "brew services start mysql",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare:shared": "rm -f bootstrap/cache/*.php && composer install --no-dev --optimize-autoloader --no-scripts && npm run build && php artisan package:discover --ansi && php artisan optimize && php artisan storage:link --force && rm -f shared-hosting.zip && zip -r shared-hosting.zip . -x 'node_modules/*' '.git/*' '.github/*' 'storage/framework/cache/data/*' 'storage/framework/sessions/*' 'storage/framework/views/*' 'storage/logs/*.log' 'tests/*' 'shared-hosting.zip'"
  },
